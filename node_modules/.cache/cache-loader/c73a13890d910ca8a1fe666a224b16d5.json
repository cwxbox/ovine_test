\nexport default () => {\n    const [state, setState] = useImmer({\n        showInput: false,\n    });\n    const history = useHistory();\n    const $searchRef = useRef(null);\n    const asideMenus = getAsideMenus();\n    const { showInput } = state;\n    const toggleInput = (toggle) => {\n        const isShow = typeof toggle === 'boolean' ? toggle : !showInput;\n        setState((d) => {\n            d.showInput = isShow;\n        });\n        if (isShow) {\n            setTimeout(() => {\n                $($searchRef.current)\n                    .find('input')\n                    .click()\n                    .focus();\n            }, 100);\n        }\n    };\n    useEffect(() => {\n        $('body').on('click', (e) => {\n            if (!$searchRef.current || $.contains($searchRef.current, e.target)) {\n                return;\n            }\n            // amis overlay 存在异常 在侧边栏 区域/顶部 区域 不会关闭\n            const $search = $($searchRef.current);\n            const $input = $search.find('input');\n            if (!$input.val()) {\n                if ($search.find('.is-opened').length) {\n                    $search.find('div[class$=\"PopOver-overlay\"]').click();\n                }\n                setState((d) => {\n                    d.showInput = false;\n                });\n            }\n        });\n    }, []);\n    const inputSchema = useMemo(() => {\n        return {\n            title: '',\n            type: 'form',\n            mode: 'normal',\n            wrapWithPanel: false,\n            className: 'm-n',\n            actions: [],\n            controls: [\n                {\n                    type: 'tree-select',\n                    name: 'nodePath',\n                    label: '',\n                    className: cls('m-n', { active: showInput }),\n                    inputClassName: 'inline',\n                    clearable: false,\n                    searchable: true,\n                    valueField: 'nodePath',\n                    placeholder